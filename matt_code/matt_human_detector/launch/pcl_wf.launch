<launch>
    <!-- Launch Gazebo -->
    <!-- Move Head Down -->

    <!-- Launch program that published a downsampled and filtered pointcloud-->
    <node pkg="matt_human_detector" type="pcl_for_nav_node" name="PCLforNavNode" output="screen">
        <rosparam file="$(find matt_human_detector)/params/pcl_for_nav_params.yaml" command = "load"/>
    </node>

    <!-- Launch python node that follows walls -->
    <node pkg="matt_human_detector" type="pcl_wf.py" name="PCL_WF_Node" output="screen"/>

    <node pkg="matt_human_detector" type="pcl_seeker_client.py" name="PCL_Client_Node" output="screen" />
        
    <node pkg="matt_human_detector" type="pcl_seeker_server.py" name="PCL_Server_Node" output="screen" />

    <arg name="map_yaml"                                                   doc="filepath to previously captured map (required)" />
    <arg name="rviz"                   default="true"                      doc="whether to show Rviz" />
    <arg name="gazebo_world"           default="worlds/willowgarage.world" doc="the environment within which Stretch is loaded in Gazebo" />
    <arg name="gazebo_gpu_lidar"       default="false"                     doc="whether to compute lidar with hardware acceleration (requires GPU)" />
    <arg name="gazebo_visualize_lidar" default="false"                     doc="whether to visualize planar lidar within Gazebo" />

    <!-- GAZEBO SIMULATION
    <include file="$(find stretch_gazebo)/launch/gazebo.launch">
        <arg name="world" value="/home/csrobot/stretch_ws/src/ARA_Summer_Robotics/Gazebo_worlds/DuckieTown_world.world" />
        <arg name="visualize_lidar" value="$(arg gazebo_visualize_lidar)" />
        <arg name="gpu_lidar" value="$(arg gazebo_gpu_lidar)" />
    </include> -->

</launch>