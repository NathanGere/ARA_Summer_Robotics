<launch>
  <!--The line below is not needed in the final version!-->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg initial_map_yaml)"/>
  
  <node name="map_saver"  pkg="map_server" type="map_saver"  args=" -f /home/csrobot/$(arg final_map_file_name) map:=/enclosed_map" output="screen"/>

  <node pkg="my_navigation" name="map_enclosed" type="map_enclosed.py" output="screen">
    <param name="~get_map_topic_name"        type="string" value="/static_map"/>
    <param name="~print_crop_data"           type="bool"   value="false"/>
    <param name="~print_is_enclosed_process" type="bool"   value="false"/>
    <param name="~print_final_terminal_map"  type="bool"   value="true"/>
  </node>
</launch>